execute pathogen#infect()
filetype plugin indent on 
syntax on
syntax enable
set tabstop=4
set expandtab
set cursorline
filetype indent on
set wildmenu
set incsearch
set hlsearch
set backspace=indent,eol,start
set number
let g:terraform_fmt_on_save=1
let g:terraform_align=1
let g:go_disable_autoinstall = 1
let g:go_fmt_autosave=1
